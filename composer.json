{
    "name": "eif/eco-int-framework",
    "type": "project",
    "license": "proprietary",
    "description": "The Eco-Interactions Framework",
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/annotations": "^1.11@dev",
        "doctrine/dbal": "^2.10",
        "doctrine/doctrine-bundle": "^2.1",
        "doctrine/doctrine-cache-bundle": "^1.4",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.7",
        "excelwebzone/recaptcha-bundle": "dev-master",
        "friendsofsymfony/rest-bundle": "^3.0",
        "friendsofsymfony/user-bundle": "2.x-dev",
        "incenteev/composer-parameter-handler": "^2.0",
        "jms/serializer-bundle": "^3.6",
        "knplabs/knp-menu-bundle": "^3",
        "sensio/framework-extra-bundle": "^5.5",
        "sentry/sentry-symfony": "^3.4",
        "stof/doctrine-extensions-bundle": "^1.4",
        "suncat/mobile-detect-bundle": "dev-master",
        "symfony/apache-pack": "dev-master",
        "symfony/console": "*",
        "symfony/dotenv": "*",
        "symfony/flex": "^1.8@dev",
        "symfony/framework-bundle": "*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/polyfill-apcu": "^1.1",
        "symfony/security-csrf": "4.4.*",
        "symfony/swiftmailer-bundle": "^3.4",
        "symfony/translation": "4.4.*",
        "symfony/twig-pack": "dev-master",
        "symfony/validator": "4.4.*",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "*",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "behat/behat": "^3.4",
        "behat/mink": "^1.8",
        "behat/mink-extension": "^2.3",
        "behat/mink-goutte-driver": "^1.2",
        "behat/mink-selenium2-driver": "^1.4",
        "behat/symfony2-extension": "^2.1",
        "dama/doctrine-test-bundle": "^6.3",
        "doctrine/data-fixtures": "^1.4",
        "hautelook/alice-bundle": "^2.7",
        "symfony/error-handler": "4.4.*",
        "symfony/phpunit-bridge": "*",
        "symfony/web-profiler-bundle": "4.4.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "minimum-stability": "dev",
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "4.4.*"
        }
    }
}
